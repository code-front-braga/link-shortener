generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Link {
  id        String   @id @default(cuid())
  title     String
  shortCode String   @unique
  longUrl   String
  click     Int      @default(0)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
